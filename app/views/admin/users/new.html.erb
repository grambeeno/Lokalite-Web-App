
<div style='width:80%;margin:auto'>

  <br />
  <br />
  <%= link_to raw('&larr; All Users'), :action => :index %>
  <br />

  <% form(@user) do |f| %>

    <%= f.error_messages %>

    <%= f.label :email %>
    <br />
    <%= f.text_field :email %>
    <br />

    <%= f.label :password %>
    <br />
    <%= f.text_field :password, :value => nil %>
    <br />

    <br />
    <%= f.submit %>

  <% end %>


</div>
