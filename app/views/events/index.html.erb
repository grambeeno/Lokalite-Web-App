<% if @events.any? %>
  <% if params[:view_type] == 'map' %>
    <%= render :partial => 'map_view' %>
  <% elsif params[:view_type] == 'list' %>
    <%= render :partial => 'list_view' %>
  <% else %>
    <%= render :partial => 'tile_view' %>
  <% end %>
<% else %>
  <%= render :partial => "no_results" %>
<% end %>

